@model E_Nompilo_Healthcare_system.Models.Information

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_FamilyPlanLayout.cshtml";
}

<h1>Create</h1>

<h4>Information</h4>
<hr />
<div class="row">
    <div class="col-md-4">
          
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Contraceptives" class="control-label"></label>
                <input asp-for="Contraceptives" class="form-control" />
                <span asp-validation-for="Contraceptives" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Info" class="control-label"></label>
                <input asp-for="Info" class="form-control" />
                <span asp-validation-for="Info" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


