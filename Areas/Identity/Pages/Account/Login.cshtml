<div class="logcontainer ">
    <div class="forms-container">
        <div class="arrow bounce">
            <a class="fa fa-arrow-left fa-2x " style="background-color:white;" asp-area="" asp-controller="Home" asp-action="Index"></a>
        </div>
        <div class="signin-signup">
            <form class="sign-in-form " id="account" method="post">
                <div asp-validation-summary="All" class="text-danger"></div>
                <h2 class="title1">Sign in</h2>
                <br>
                <div class="mb-3 ">
                    <label for="validationCustomUsername" class="form-label">Username</label>
                    <div class="input-group has-validation">
                        <span class="input-group-text" id="inputGroupPrepend">@@</span>
                        <input asp-for="Input.Email" type="email" class="form-control" placeholder="example@gmail.com" id="validationCustomUsername" aria-describedby="inputGroupPrepend" required>
                        <span asp-validation-for="Input.Email" class="text-danger"></span>
                        <div class="invalid-feedback">
                            Please Provide a username.
                        </div>
                    </div>
                </div>

                <div class="mb-3">
                    <label for="validationCustom03" class="form-label">Password</label>
                    <div class="input-group has-validation">

                        <input asp-for="Input.Password" type="password" class="form-control" id="validationCustom03" autocomplete="new-password" aria-describedby="inputGroupPrepend" required>
                        <span asp-validation-for="Input.Password" class="text-danger"></span>
                        <div asp-for="Input.Password" class="invalid-feedback">
                            Please Provide a Password.
                        </div>
                    </div>
                </div>

                <div class="checkbox">
                    <label asp-for="Input.RememberMe" class="form-label">
                        <input class="form-check-input" asp-for="Input.RememberMe" />
                        @Html.DisplayNameFor(m => m.Input.RememberMe)
                    </label>
                </div>

                <input id="login-submit" type="submit" value="Login" class="btnn" />
                <a class="nav-link text-decoration-underline" asp-area="Identity" asp-page="/Account/ForgotPassword">Forgot Password</a>
                <p class="social-text">Or Sign with social plartforms</p>
                <div class="social-media">
                    <a href="#" class="social-icon">
                        <i class="fab fa-facebook-f"></i>
                    </a>
                    <a href="#" class="social-icon">
                        <i class="fab fa-twitter"></i>
                    </a>
                    <a href="#" class="social-icon">
                        <i class="fab fa-google"></i>
                    </a>
                    <a href="#" class="social-icon">
                        <i class="fab fa-linkedin-in"></i>
                    </a>
                </div>
            </form>



        </div>
    </div>
    @*<img src="~/images/12.png" class="h-25 w-25" style="filter:brightness(120%);" alt="">*@
    <div class="panels-container">
        <div class="panel left-panel">
            <div class="content">
                <h3>New here ?</h3>
                <p>
                    Get Better with us by signing up ,Health is not valued till sickness comes. <br>

                    <a class=" right_btn " style=" font-size:18px; color:white;" id="sign-up-btn" asp-area="Identity" asp-page="/Account/Register"><i class="fa fa-fw fa-user"></i>Sign Up</a>

                </p>
            </div>

            <img src="~/images/undraw_sign_in_re_o58h.svg" class="image " alt="">
        </div>



    </div>
</div>
<script>
    (function () {
        'use strict'

        // Fetch all the forms we want to apply custom Bootstrap validation styles to
        var forms = document.querySelectorAll('.needs-validation')

        // Loop over them and prevent submission
        Array.prototype.slice.call(forms)
            .forEach(function (form) {
                form.addEventListener('submit', function (event) {
                    if (!form.checkValidity()) {
                        event.preventDefault()
                        event.stopPropagation()
                    }

                    form.classList.add('was-validated')
                }, false)
            })
    })()
</script>

